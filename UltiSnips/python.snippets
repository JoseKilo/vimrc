snippet spfa "Shortest path faster algorithm" !b
INF = 10 ** 100


def process_case(V, start, end, edges):
    nodes = defaultdict(dict)
    for u, v, w in edges:
        nodes[u][v] = int(w)

    dist = defaultdict(lambda: INF)
    dist[start] = 0
    in_queue = {start}
    times = Counter()
    times[start] += 1

    queue = [start]
    while queue:
        u = queue.pop()
        in_queue.discard(u)

        for v, w in nodes[u].items():
            if dist[u] + w < dist[v]:
                dist[v] = dist[u] + w
                if v not in in_queue:
                    queue.insert(0, v)
                    in_queue.add(v)
                    times[v] += 1

                    if times[v] >= V:
                        return 'BAZINGA'

    result = dist[end]

    return result
endsnippet

